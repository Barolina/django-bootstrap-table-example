{
  "name": "django-bootstrap-table-example",
  "version": "0.1.0",
  "description": "An example django app using bootstrap-table, mithril.js and django-rest-framework-json-api",
  "main": "index.js",
  "dependencies": {
    "URIjs": "^1.16.0",
    "bootstrap": "^3.3.5",
    "bootstrap-table": "^1.8.1",
    "jquery": "^1.11.3",
    "jquery.cookie": "^1.4.1",
    "mithril": "^0.2.0",
    "pubsub-js": "^1.5.2"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "licensify": "^1.4.0"
  },
  "browser": {
    "bootstrap-table": "./build/bootstrap-table-all.js",
    "bootstrap-table-ja": "./node_modules/bootstrap-table/dist/locale/bootstrap-table-ja-JP.js"
  },
  "scripts": {
    "prelicensify": "sed '1s|^/\\*\\*|/*!|' ./node_modules/bootstrap-table/dist/bootstrap-table-all.js > ./build/bootstrap-table-all.js",
    "licensify": "browserify project/bookmarks/static_source/js/app.js -p [ licensify --scanBrowser ] -o build/app.js",
    "minify:js": "uglifyjs build/app.js -o project/bookmarks/static/js/app.js --comments '/generated by licensify/'",
    "prebuild": "mkdir -p build project/bookmarks/static/js",
    "build": "npm run prebuild && npm run prelicensify && npm run licensify && npm run minify:js",
    "watchify": "watchify project/bookmarks/static_source/js/app.js -o project/bookmarks/static/js/app.js -p licensify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hnakamur/django-bootstrap-table-example.git"
  },
  "keywords": [
    "django",
    "twitter-bootstrap",
    "json-api"
  ],
  "author": "Hiroaki Nakamura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hnakamur/django-bootstrap-table-example/issues"
  },
  "homepage": "https://github.com/hnakamur/django-bootstrap-table-example#readme"
}
